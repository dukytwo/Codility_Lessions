    "def solution(S):\n",
    "    \n",
    "    s = list(S)\n",
    "    n = len(s)\n",
    "    \n",
    "    if n%2 == 0:\n",
    "        return -1\n",
    "    \n",
    "    mid = n//2\n",
    "    j = 1\n",
    "    while (mid+j < n):\n",
    "        if s[mid-j] != s[mid+j]:            \n",
    "            return -1\n",
    "        j += 1\n",
    "        \n",
    "    return mid"
   ]
  }
