    "def gcd(a,b):\n",
    "    if a%b != 0:\n",
    "        return gcd(b, a%b)\n",
    "    else:\n",
    "        return b\n",
    "    \n",
    "def solution(N, M):\n",
    "    gcd_NM = gcd(N, M)\n",
    "    lcm_NM = N*M/gcd_NM\n",
    "    return int(lcm_NM/M)"
   ]
