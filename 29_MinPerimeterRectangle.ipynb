    "def solution(N):\n",
    "    \n",
    "    i = 1\n",
    "    max_factor = 1\n",
    "    while (i * i < N):\n",
    "        if N%i == 0:\n",
    "            max_factor = i\n",
    "        i += 1\n",
    "    \n",
    "    if i * i == N:\n",
    "        max_factor = i\n",
    "    \n",
    "    a = max_factor\n",
    "    b= N/a\n",
    "    \n",
    "    return int(2*(a+b))"
   ]
  }
