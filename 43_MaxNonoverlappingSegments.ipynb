   "source": [
    "def solution(A, B):\n",
    "    n = len(A)  \n",
    "    \n",
    "    if n == 0:\n",
    "        return 0    \n",
    "    \n",
    "    count = 1\n",
    "    current_end = B[0]\n",
    "    for i in range(1,n):\n",
    "        if A[i] > current_end:\n",
    "            count += 1\n",
    "            current_end = B[i]\n",
    "            \n",
    "    return count"
   ]
  }
