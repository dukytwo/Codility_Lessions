   "source": [
    "def solution(T):\n",
    "    if T.l == None and T.r == None:\n",
    "        return 0\n",
    "    elif T.l == None:\n",
    "        return 1 + solution(T.r)\n",
    "    elif T.r == None:\n",
    "        return 1 + solution(T.l)\n",
    "    else:\n",
    "        return 1+ max(solution(T.l), solution(T.r))"
   ]
  }
