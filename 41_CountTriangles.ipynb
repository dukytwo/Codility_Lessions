   "source": [
    "def solution(A):\n",
    "    n = len(A)\n",
    "    n_tri = 0\n",
    "    A.sort()\n",
    "    for x in range(n):\n",
    "        z= x + 2\n",
    "        for y in range(x+1, n):\n",
    "            while (z < n and A[x] + A[y] > A[z]):\n",
    "                z += 1\n",
    "                n_tri += z - y - 1\n",
    "    return n_tri"
   ]
  }
