    "def solution(A):\n",
    "    \n",
    "    N = len(A)\n",
    "    if N == 0:\n",
    "        return 0\n",
    "    \n",
    "    A.sort()    \n",
    "    unique_count = 1\n",
    "    \n",
    "    for i in range(1, N):\n",
    "        if A[i] != A[i-1]:\n",
    "            unique_count += 1\n",
    "        \n",
    "    return  unique_count"
   ]
  }
