    "def solution(A):\n",
    "    N = len(A)\n",
    "    triangle_exist = 0\n",
    "    \n",
    "    if N < 3:\n",
    "        triangle_exist = 0\n",
    "    \n",
    "    A.sort(reverse = True)\n",
    "    \n",
    "    for i in range(N-2):\n",
    "        if A[i] < A[i+1] + A[i+2]:\n",
    "            triangle_exist = 1\n",
    "    \n",
    "    return triangle_exist "
   ]
  }
